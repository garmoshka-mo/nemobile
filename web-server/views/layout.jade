  body(ng-controller='mainController')
    .snap-drawers
      .snap-drawer.snap-drawer-left
        aside.left-off-canvas-menu
          .iphone-hack.iphone-menu-margin
          div(ng-include="'partials/menu.html?'+version", ng-controller='menuController')
    .off-canvas-wrap.snap-content(slide-menu='', data-offcanvas='')
      .prevent-click
      .iphone-hack.iphone-status-bar-margin
      .inner-wrap
        nav.tab-bar.no-select
          section.left-small.pointer.ta-center
            a.menu-icon(ng-hide='isBackArrownShown || customSmallIconInner', ng-click='toggleMenu()')
              span
            i.fa.fa-chevron-left.ml10.pointer(ng-show='isBackArrownShown && !customSmallIconInner', ng-click='backArrowHandler()')
            span.customSmallIcon(ng-show='customSmallIconInner', ng-click='customSmallIconHandler()', ng-bind-html='customSmallIconInner')
          section.middle.tab-bar-section.p0
            h1.title
              div(ng-show='routing.is_preload')
                span(spinner='', spinner-color='white')
              div(ng-hide='notification.text || routing.is_preload', ui-view='title')
              .notification(ng-show='notification.text && !routing.is_preload', ng-click='notification.handler()', ng-class="{'animated fadeIn': notification.animated}")
                span.pointer
                  img.chat-toolbar-image.pointer(ng-show='notification.ava_url', ng-src='{{notification.ava_url}}')
                  span(ng-bind='notification.text')
                  span.typing(ng-show='notification.typing', ng-bind='notification.typing_label')
                a.options
                  i.fa.fa-ellipsis-v
          section.right-small.chat-right-tool(ng-cloak='', ng-show='true')
            .publish
              a.icon_button(ng-click='publish()', title='Предварительный просмотр для публикации чата')
                i.fa.fa-share-alt
            .time_zone(ng-show='notification.time')
              i.fa.fa-clock-o
              span(ng-bind='notification.time')
              .dubink DUB.iNK
          section.right-small.ta-center(ng-cloak='', ng-show='user.score && isUserScoreShown')
            i.fa.fa-star
            span(ng-bind='user.score')
        section.main-section(ng-class="{'brand_bg': isChatState()}")
          // content goes here
          // MAIN CONTENT
          #wrapper(ng-hide='routing.is_preload')
            .content(ui-view='content')
              block content
        a.exit-off-canvas
