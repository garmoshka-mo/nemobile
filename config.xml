<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.dubink" version="1.4.3" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Dub.ink</name>
    <description>
        Chat
    </description>
    <author email="support@dub.ink" href="http://dub.ink">
        DUB.iNK
    </author>
    <content src="launcher.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="cli-5.2.0" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="false" />
    <preference name="auto-hide-gap:splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="KeyboardShrinksView" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="android-windowSoftInputMode" value="stateHidden|adjustResize" />
    <preference name="fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="7000" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="fade-splash-screen-duration" value="5" />
    <preference name="android-build-tool" value="gradle" />
    <gap:plugin name="cordova-plugin-device" source="npm" version="1.0.1" />
    <gap:plugin name="com.ququplay.websocket.websocket" version="0.1.0" />
    <gap:plugin name="cordova-plugin-inappbrowser" source="npm" version="1.0.1" />
    <gap:plugin name="cordova-plugin-media" source="npm" version="1.0.1" />
    <gap:plugin name="phonegap-plugin-push" source="npm" version="1.3.0" />
    <gap:plugin name="cordova-plugin-splashscreen" source="npm" version="2.1.0" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" version="1.0.0" />
    <gap:plugin name="cordova-plugin-file" source="npm" version="3.0.0" />
    <gap:plugin name="cc.fovea.cordova.purchase" source="npm" version="4.0.0">
        <param name="BILLING_KEY" value="" />
    </gap:plugin>
    <gap:config-file mode="delete" parent="/manifest" platform="android">
        <uses-permission android:name="android.permission.RECORD_AUDIO" />
        <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <uses-permission android:name="android.permission.READ_PHONE_STATE" />
        <uses-permission android:name="android.permission.READ_CONTACTS" />
    </gap:config-file>
    <gap:plugin name="com.danielcwilson.plugins.googleanalytics" version="0.6.1" />
    <gap:plugin name="ionic-plugin-keyboard" source="npm" version="1.0.7" />
    <gap:plugin name="nl.x-services.plugins.socialsharing" version="4.3.0" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="res/icon/android/drawable-ldpi/appicon.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="res/icon/android/drawable-mdpi/appicon.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="res/icon/android/drawable-hdpi/appicon.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="res/icon/android/drawable-xhdpi/appicon.png" />
    <icon gap:platform="android" gap:qualifier="xxhdpi" src="res/icon/android/drawable-xxhdpi/appicon.png" />
    <icon gap:platform="android" gap:qualifier="xxxhdpi" src="res/icon/android/drawable-xxxhdpi/appicon.png" />
    <icon gap:platform="ios" height="180" src="res/icon/ios/appicon-60@3x.png" width="180" />
    <icon gap:platform="ios" height="60" src="res/icon/ios/appicon-60.png" width="60" />
    <icon gap:platform="ios" height="120" src="res/icon/ios/appicon-60@2x.png" width="120" />
    <icon gap:platform="ios" height="76" src="res/icon/ios/appicon-76.png" width="76" />
    <icon gap:platform="ios" height="152" src="res/icon/ios/appicon-76@2x.png" width="152" />
    <icon gap:platform="ios" height="40" src="res/icon/ios/icon-40.png" width="40" />
    <icon gap:platform="ios" height="80" src="res/icon/ios/icon-40@2x.png" width="80" />
    <icon gap:platform="ios" height="57" src="res/icon/ios/appicon.png" width="57" />
    <icon gap:platform="ios" height="114" src="res/icon/ios/appicon@2x.png" width="114" />
    <icon gap:platform="ios" height="72" src="res/icon/ios/appicon-72.png" width="72" />
    <icon gap:platform="ios" height="144" src="res/icon/ios/appicon-72@2x.png" width="144" />
    <icon gap:platform="ios" height="29" src="res/icon/ios/appicon-Small.png" width="29" />
    <icon gap:platform="ios" height="58" src="res/icon/ios/appicon-Small@2x.png" width="58" />
    <icon gap:platform="ios" height="87" src="res/icon/ios/appicon-Small@3x.png" width="87" />
    <icon gap:platform="ios" height="50" src="res/icon/ios/appicon-Small-50.png" width="50" />
    <icon gap:platform="ios" height="100" src="res/icon/ios/appicon-Small-50@2x.png" width="100" />
    <gap:splash gap:platform="android" gap:qualifier="hdpi" src="res/screen/android/drawable-hdpi/screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="ldpi" src="res/screen/android/drawable-ldpi/screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="mdpi" src="res/screen/android/drawable-mdpi/screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="xhdpi" src="res/screen/android/drawable-xhdpi/screen.png" />
    <gap:splash gap:platform="android" gap:qualifier="xxhdpi" src="res/screen/android/drawable-xxhdpi/screen.png" />
    <gap:splash gap:platform="ios" height="480" src="res/screen/ios/Default~iPhone.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/ios/Default@2x~iphone.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="res/screen/ios/Default-568h@2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1334" src="res/screen/ios/Default-667h@2x.png" width="750" />
    <gap:splash gap:platform="ios" height="2208" src="res/screen/ios/Default-Portrait-736h@3x.png" width="1242" />
    <gap:config-file parent="/manifest/application" platform="android">
        <activity android:launchMode="singleTask" />
    </gap:config-file>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <engine name="ios" spec="~3.9.2" />
</widget>
